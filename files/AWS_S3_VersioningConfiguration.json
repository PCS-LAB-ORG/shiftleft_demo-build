{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Resources": {
    "s3manishcicd": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "Tags": [
          {
            "Key": "git_repo",
            "Value": "shiftleft_demo-build"
          },
          {
            "Key": "git_org",
            "Value": "PCS-LAB-ORG"
          }
        ],
        "AccessControl": "LogDeliveryWrite",
        "LoggingConfiguration": {
          "DestinationBucketName": {
            "Ref": "s3manishcicd"
          }
        },
        "VersioningConfiguration": {
          "Status": "Disabled"
        }
      }
    }
  },
  "Description": "Example for ECS cicd policy"
}
