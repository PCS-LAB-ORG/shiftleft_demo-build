{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "InstanceSecurityGroup": {
            "Type": "AWS::EC2::SecurityGroup",
            "Properties": {
                "Tags": [
                  {
                    "Key": "git_repo",
                    "Value": "shiftleft_demo-build"
                  },
                  {
                    "Key": "git_org",
                    "Value": "PCS-LAB-ORG"
                  }
                ],
                "GroupDescription": "Allow http to client host",
                "VpcId": "myVPC",
                "GroupName" : "default",
                "SecurityGroupIngress": [{
                    "IpProtocol": "-1",
                    "CidrIp": "0.0.0.0/0",
                    "CidrIpv6" : "::/0"
                }],
                "SecurityGroupEgress": [{
                    "IpProtocol": "-1",
                    "CidrIp": "0.0.0.0/0",
                    "CidrIpv6" : "::/0"
                }]
            }
        }
    }
}
